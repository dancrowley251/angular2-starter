{
  "name": "angular2webpack-starter",
  "version": "1.3.2",
  "description": "A simple angular project which will allow you to start bulding angluar2 applications quickly.",
  "main": "app.js",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start karma.config.js",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BillBensing/Angular2-Webpack-Starter.git"
  },
  "keywords": [
    "angular2",
    "angular",
    "webpack",
    "starter"
  ],
